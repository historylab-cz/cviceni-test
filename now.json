{
  "version": 2,
  "routes": [
    {
      "src": ".*/assets/(.*)",
      "headers": {
        "cache-control": "public, s-maxage=86400, max-age=0, immutable"
      }
    },
    {
      "src": "/dev/([^/]+)$",
      "status": 301,
      "headers": { "Location": "/dev/$1/" }
    },
    {
      "src": "/cviceni/([^/]+)$",
      "status": 301,
      "headers": { "Location": "/cviceni/$1/" }
    },
    {
      "src": "/features/([^/]+)$",
      "status": 301,
      "headers": { "Location": "/features/$1/" }
    }
  ]
}
