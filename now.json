{
  "version": 2,
  "routes": [
    {
      "src": ".*/assets/(.*)",
      "headers": {
        "cache-control": "public, s-maxage=86400, max-age=0, immutable"
      }
    },
    { 
      "src": "/(((?!\\?|#|\\.).)*((?!\\?|/|#|\\.).))", 
      "status": 301,
      "headers": { "Location": "/$1/" }
    },
  ]
}
